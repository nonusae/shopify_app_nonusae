{% layout none %}
{% capture output %} 
            {% if collections['all'].all_tags.size > 0 %}
                {% for tag in collections['all'].all_tags %}
                    {% if forloop.index == 1 %}""{% endif %}
                 
                    {% if output contains tag %}
                    {% else %}
                        ,"{{tag}}"
                    {% endif %}
     
                {% endfor %}  
            {% endif %}     
{% endcapture %}
{{ output | strip_newlines | prepend: '[' | append: ']' }}